<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>VerificarUsuario</servlet-name>
        <servlet-class>Servlet.VerificarUsuario</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>VerifiicarGerente</servlet-name>
        <servlet-class>Servlet.VerifiicarGerente</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Guardar_Cliente</servlet-name>
        <servlet-class>Servlet.Guardar_Cliente</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Guardar_Empleado</servlet-name>
        <servlet-class>Servlet.Guardar_Empleado</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Editar_Cliente</servlet-name>
        <servlet-class>Servlet.Editar_Cliente</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>VerificarUsuario</servlet-name>
        <url-pattern>/VerificarUsuario</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>VerifiicarGerente</servlet-name>
        <url-pattern>/VerifiicarGerente</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Guardar_Cliente</servlet-name>
        <url-pattern>/Guardar_Cliente</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Guardar_Empleado</servlet-name>
        <url-pattern>/Guardar_Empleado</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Editar_Cliente</servlet-name>
        <url-pattern>/Editar_Cliente</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
